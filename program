#include <iostream>
#include <math.h>
#include <iomanip>

using namespace std;

int main()
{
	double a, b, h, y, s, p, x;
	int n, i;
	cout << "Vvedite a,b,n" << endl;
	cin >> a >> b >> n;
	h = (b - a) / 10;
	for (x = a; x <= b; x += h)
	{
		p = x;
		s = x;
		for (i = 1; i <= n; i++)
		{
			p *=( x * x )/((2*i)*(2*i +1));
			s += p;
		}
		y = (exp(x) - exp(-x))/2 ;
		cout << setw(45) << y << setw(45) << s << endl;
		cout.setf(ios::fixed);
        cout.precision(30);
	}
	return 0;
}
