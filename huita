#include <iostream>

using namespace std;

int main()
{
	int i, j, t;
	const int size = 10;
	int el[size];
	int al[3]{};
	t = 0;
	int MAX = 3;
	for (i = 0; i < size; i++) {
		el[i] = rand() % MAX;
	}
	
	for (i = 0; i < size; i++)
	{
		cout << el[i] << " ";
	}
 
	cout << endl;

	for (i = 0; i < size; i++)
	{
		for (j = 0; j < size - 1; j++)
		{
			if (el[j] > el[j + 1])
			{
				swap(el[j], el[j + 1]);
			}

		}
	}
	for (i = 0; i < size; i++)
	{
		cout << el[i];
	}
	cout << endl;
	int q = 0;  i = 0;
	int a = 0;
	
	for (j = 0 ; j < size; j++) 
	{
		if (el[j] == el[j + 1])
			{
				al[i]++;
			}
		else {
			i++;
		}
	}
	
	for(i = 0; i < 3; i++) {
		cout << al[i] + 1 << " --- " << " chisel " << i << endl;
	}
	return 0;
}
